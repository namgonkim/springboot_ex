{{>layout/header}}
    <h1>공공데이터 API 서비스</h1>
    <div class="col-md-12">
        <div class="row">
            <!-- 모달 버튼 -->
            <div class="col-md-6">
                <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#savePostsModal">서비스 실행</button>
            </div>

            <!-- 모달 버튼 클릭시 -->
            <div class="modal fade" id="savePostsModal" tabindex="-1" role="dialog" aria-labelledby="savePostsLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="savePostsLabel">공공데이터 API 서비스</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <form>
                                <div class="form-group">
                                    <label for="pageNo">페이지 번호</label>
                                    <input type="text" class="form-control" id="pageNo" placeholder="페이지 번호를 입력하세요">
                                </div>

                                <div class="form-group">
                                    <label for="numOfRows">한 페이지 결과 수</label>
                                    <input type="text" class="form-control" id="numOfRows" placeholder="한 페이지 결과 수를 입력하세요">
                                </div>
                                <div class="form-group">
                                    <label for="startCreateDt">검색할 생성일 범위[시작]</label>
                                    <input type="text" class="form-control" id="startCreateDt" placeholder="검색할 생성일 범위[시작]">
                                </div>
                                <div class="form-group">
                                    <label for="endCreateDt"> 검색할 생성일 범위[끝] </label>
                                    <input type="text" class="form-control" id="endCreateDt" placeholder="검색할 생성일 범위[끝]">
                                </div>
                            </form>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">취소</button>
                            <button type="button" class="btn btn-primary" id="btn-find">조회</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="mainContainer">
            <div class="col-xs-1 text-center" id="APIData"></div>
        </div>
    </div>
{{>layout/footer}}